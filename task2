#include <iostream>

using namespace std;

int main()
{
    int res;
    cout<<"Enter your result: ";
    cin>>res;
    if(res>=90 && res<=100){
        cout<<"A"<<endl;
    }
    else if(res>=80 && res<=89){
        cout<<"B"<<endl;
    }
    else if(res>=70 && res<=79){
        cout<<"C"<<endl;
    }
    else if(res>=60 && res<=69){
        cout<<"D"<<endl;
    }
    else if(res>=0 && res<=59){
        cout<<"F"<<endl;
    }
    else{
        cout<<"invalid input";
    }

    return 0;
}
